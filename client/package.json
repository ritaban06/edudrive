    "@react-pdf-viewer/default-layout": "^3.12.0",
    "@react-pdf-viewer/full-screen": "^3.12.0",
    "@react-pdf-viewer/toolbar": "^3.12.0",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "axios": "^1.11.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.454.0",
    "pdfjs-dist": "3.11.174",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-file-viewer": "^1.2.1",
